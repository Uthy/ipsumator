{
    "hosting": {
        "public": "public",
        "headers": [{
                "source": "/**",
                "headers": [{
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }]
            },
            {
                "source": "**/*.@(eot|otf|ttf|ttc|woff)",
                "headers": [{
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }]
            },
            {
                "source": "**/*.@(jpg|jpeg|gif|png)",
                "headers": [{
                    "key": "Cache-Control",
                    "value": "max-age=7200"
                }]
            }
        ]
    }
}